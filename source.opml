					<outline text="httpRequest (urlOpml, undefined, undefined, function (err, opmltext) {" isComment="true">
						<outline text="if (err) {">
							<outline text="console.log (&quot;viewOutline: urlOpml == &quot; + urlOpml + &quot;, err.message == &quot; + err.message);"/>
							<outline text="callback (err);"/>
							<outline text="}"/>
							</outline>
						<outline text="else {">
							<outline text="console.log (&quot;viewOutline: urlOpml == &quot; + urlOpml + &quot;, secs == &quot; + secondsSince (whenstart)); "/>
							<outline text="opInitOutliner (opmltext, true, true);"/>
							<outline text="if (callback !== undefined) {" created="Thu, 20 Jul 2023 15:22:15 GMT">
								<outline text="callback ();" created="Thu, 20 Jul 2023 15:22:06 GMT"/>
								<outline text="}" created="Thu, 20 Jul 2023 15:22:23 GMT"/>
								</outline>
							<outline text="}"/>
							</outline>
						<outline text="});"/>
						</outline>
					<outline text="}" created="Thu, 20 Jul 2023 15:11:57 GMT"/>
					</outline>
