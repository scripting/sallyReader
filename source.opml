				<outline text="function viewOutline (urlOpml, callback) {" created="Thu, 20 Jul 2023 15:11:42 GMT">
					<outline text="var whenstart = new Date ();"/>
					<outline text="readOutline (urlOpml, function (err, opmltext) {" created="Fri, 21 Jul 2023 13:51:17 GMT">
						<outline text="if (err) {">
							<outline text="console.log (&quot;viewOutline: urlOpml == &quot; + urlOpml + &quot;, err.message == &quot; + err.message);"/>
							<outline text="callback (err);"/>
							<outline text="}"/>
							</outline>
						<outline text="else {">
